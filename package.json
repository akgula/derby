{
  "name": "derby",
  "description": "MVC framework making it easy to write realtime, collaborative applications that run in both Node.js and browsers.",
  "version": "2.2.0",
  "homepage": "http://derbyjs.com/",
  "repository": {
    "type": "git",
    "url": "git://github.com/derbyjs/derby.git"
  },
  "main": "index.js",
  "files": [
    "dist/**/*.js"
  ],
  "scripts": {
    "build": "node_modules/.bin/tsc",
    "checks": "npm run lint && npm test",
    "lint": "npx eslint *.js lib/**/*.js test/**/*.js test-utils/**/*.js",
    "prepare": "npm run build",
    "test": "npx mocha 'test/all/**/*.mocha.js' 'test/dom/**/*.mocha.js' 'test/server/**/*.mocha.js'",
    "test-browser": "node test/server.js"
  },
  "dependencies": {
    "chokidar": "^3.5.3",
    "esprima-derby": "^0.1.0",
    "html-util": "^0.2.3",
    "qs": "^6.11.0",
    "racer": "^1.0.3",
    "resolve": "^1.22.1",
    "serialize-object": "^1.0.0",
    "through": "^2.3.8",
    "tracks": "^0.5.8"
  },
  "devDependencies": {
    "@types/node": "^20.3.1",
    "async": "^3.2.4",
    "browserify": "^17.0.0",
    "chai": "^4.3.6",
    "eslint": "^8.37.0",
    "express": "^4.18.1",
    "jsdom": "^20.0.1",
    "mocha": "^10.0.0",
    "typescript": "^5.1.3"
  },
  "optionalDependencies": {},
  "bugs": {
    "url": "https://github.com/derbyjs/derby/issues"
  },
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "author": "Nate Smith",
  "license": "MIT"
}
